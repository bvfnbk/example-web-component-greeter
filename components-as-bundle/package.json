{
  "name": "components-as-bundle",
  "version": "1.0.0",
  "description": "Distributing a browser bundle.",
  "main": "index.js",
  "scripts": {
    "karma": "karma start",
    "clean": "shx rm -rf dist",
    "prepare": "shx mkdir -p dist",
